#include<stdio.h>
#include<conio.h>
#define MAX 25
typedef struct stack
{
    int data[MAX];
    int top;
}stack;
void initial(stack *);
int empty(stack *);
int full(stack *);
int pop(stack *);
void push(stack *,int x);
int evaluate(char x,int op1,int op2);
int main()
{
    stack s;
    char x;
    int op1,op2,val;
    initial(&s);
    printf("Enter expression: \n");
    while((x=getchar())!='n')
    {
        {
            op2=pop(&s);
            op1=pop(&s);
            val=evaluate(x,op1,op2);
            push(&s,val);
        }
    }
    val=pop(&s);
printf("Value of expression = %d",val);
}
void push(stack *s,int x)
{
    s->top=s->top+1;
    s->data[s->top]=x;
}
int pop(stack *s)
{
    int x;
    x=s->data[s->top];
    s->top=s->top-1;
    return(x);
}
int evaluate(char x,int op1,int op2)
{
    if(x=='+')
        return(op1+op2);
    if(x=='-')
        return(op1-op2);
    if(x=='*')
        return(op1*op2);
    if(x=='/')
            return(op1/op2);
    if(x=='%')
            return(op1%op2);
}
void initial(stack *s)
{
    s->top=-1;
}
int full(stack *s)
{
    if(s->top==MAX-1)
        return(1);
    return(0);
}
int empty(stack *s)
{
    if(s->top==-1)
        return(1);
    return(0);
}


